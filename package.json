{
  "name": "vant-theme",
  "version": "1.0.0",
  "description": "online theme preview built on Vant UI",
  "license": "MIT",
  "author": {
    "name": "aisen60",
    "email": "caixusheng60@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Aisen60/vant-theme.git"
  },
  "bugs": {
    "url": "https://github.com/Aisen60/vant-theme/issues"
  },
  "homepage": "https://github.com/Aisen60/vant-theme#readme",
  "keywords": [
    "vue",
    "vue3",
    "vant 2",
    "vant 3",
    "vant 4",
    "theme",
    "vant-theme"
  ],
  "type": "module",
  "scripts": {
    "dev": "npm run serve",
    "serve": "cross-env VANT_THEME_BASE=/ node scripts/serve.js",
    "build": "cross-env VANT_THEME_BASE=/ node scripts/build.js",
    "build:gh-pages": "cross-env VANT_THEME_BASE=/vant-theme/ node scripts/build.js",
    "serve:vite": "vite",
    "build:vite": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@vicons/ionicons5": "^0.12.0",
    "@vueuse/core": "^10.7.2",
    "dayjs": "^1.11.10",
    "jszip": "^3.10.1",
    "naive-ui": "^2.37.3",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "vue": "^3.3.11",
    "vue-i18n": "9",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^18.19.3",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.0",
    "autoprefixer": "^10.4.16",
    "cross-env": "^7.0.3",
    "download-git-repo": "^3.0.2",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.29.1",
    "fs-extra": "^11.2.0",
    "npm-run-all2": "^6.1.1",
    "ora": "^8.0.1",
    "postcss": "^8.4.33",
    "prettier": "^3.0.3",
    "puppeteer": "^13.4.0",
    "sass": "^1.69.7",
    "tailwindcss": "^3.4.1",
    "typescript": "~5.3.0",
    "unplugin-auto-import": "^0.17.3",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.0.10",
    "vite-plugin-compression": "^0.5.1",
    "vue-tsc": "^1.8.25"
  },
  "engines": {
    "node": ">=16",
    "pnpm": ">= 8"
  }
}